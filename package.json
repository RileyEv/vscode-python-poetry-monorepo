{
  "name": "poetry-monorepo",
  "displayName": "Poetry Monorepo",
  "description": "Helps to set proper interpreter and add package path to work properly with poetry on a monorepo, where there are multiple poetry projects",
  "publisher": "RileyEv",
  "repository": {
    "type": "git",
    "url": "https://github.com/RileyEv/vscode-python-poetry-monorepo.git"
  },
  "icon": "icon.png",
  "keywords": [
    "poetry",
    "monorepo",
    "python",
    "extraPaths",
    "interpreter",
    "environment",
    "venv"
  ],
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:python",
    "workspaceContains:pyproject.toml"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [],
    "languages": [
      {
        "id": "python",
        "extensions": [".py"]
      }
    ],
    "configuration": {
      "properties": {
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "extensionDependencies": [
    "ms-python.python"
  ],
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/vscode": "^1.85.0",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.8",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@vscode/python-extension": "^1.0.5"
  }
}
